    <div style="align-items:center">
    <h1 class="post-title" style="text-align:center">Come meet the dogs!</h1>
    <h5 style="width:50%; margin-left:26%; text-align:center; background-color: rgba(255,247,232,0.5); padding: 10px">Choose your dog breed!</h5>
    </div>
<div class="row">

  <!-- Grid column -->
  <div class="col-md-12 d-flex justify-content-center mb-5">  
    <button type="button" class="btn btn-outline-black waves-effect filter" data-rel="all">All</button>
    <button type="button" class="btn btn-outline-black waves-effect filter" data-rel="beagle">Beagle</button>
    <button type="button" class="btn btn-outline-black waves-effect filter" data-rel="corgi">Corgi</button>
    <button type="button" class="btn btn-outline-black waves-effect filter" data-rel="dachshund">Dachshund</button>
    <button type="button" class="btn btn-outline-black waves-effect filter" data-rel="golden">Golden Retriever</button>
    <button type="button" class="btn btn-outline-black waves-effect filter" data-rel="husky">Husky</button>
    <button type="button" class="btn btn-outline-black waves-effect filter" data-rel="schnauzer">Schnauzer</button>

  </div>
  <!-- Grid column -->

</div>
<!-- Grid row -->

<!-- Grid row -->
<div class="gallery" id="gallery">
  
  <!-- Grid column -->
  {{#each db}}
  <div class="mb-3 pics animation all {{breed}}" data-id="{{_id}}">
      <button class = "edit" style="border:0px; background-color:transparent" data-id="{{_id}}">
      <a class="custom-card">
      <figure class="figure">
        <img src="../uploads/{{image}}" class="figure-img img-fluid rounded">
        <figcaption class="figure-caption text-center dog_name">{{name}}</figcaption>
      </figure>
          </a></button>
  </div>
  {{/each}}  
